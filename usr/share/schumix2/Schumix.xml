<?xml version="1.0"?>
<Schumix>
    <Server>
        <Enabled>False</Enabled>
        <Host>127.0.0.1</Host>
        <Port>35220</Port>
        <Password>schumix</Password>
    </Server>
    <Irc>
        <Server>irc.rizon.net</Server>
        <Port>6667</Port>
        <Ssl>False</Ssl>
        <NickName>Schumix2</NickName>
        <NickName2>_Schumix2</NickName2>
        <NickName3>__Schumix2</NickName3>
        <UserName>Schumix2</UserName>
        <UserInfo>Schumix2 IRC Bot</UserInfo>
        <MasterChannel>#schumix2</MasterChannel>
        <IgnoreChannels> </IgnoreChannels>
        <IgnoreNames> </IgnoreNames>
        <NickServ>
            <Enabled>False</Enabled>
            <Password>password</Password>
        </NickServ>
        <HostServ>
            <Enabled>False</Enabled>
            <Vhost>False</Vhost>
        </HostServ>
        <Wait>
            <MessageSending>400</MessageSending>
        </Wait>
        <Command>
            <Prefix>$</Prefix>
        </Command>
        <MessageType>Privmsg</MessageType>
    </Irc>
    <Log>
        <FileName>Schumix.log</FileName>
        <LogLevel>2</LogLevel>
        <LogDirectory>$home/.Schumix2/Logs</LogDirectory>
        <IrcLogDirectory>$home/.Schumix2/Channels</IrcLogDirectory>
        <IrcLog>False</IrcLog>
    </Log>
    <MySql>
        <Enabled>False</Enabled>
        <Host>localhost</Host>
        <User>root</User>
        <Password>password</Password>
        <Database>database</Database>
        <Charset>utf8</Charset>
    </MySql>
    <SQLite>
        <Enabled>true</Enabled>
        <FileName>$home/.Schumix2/Schumix.db3</FileName>
    </SQLite>
    <Addons>
        <Enabled>True</Enabled>
        <Ignore>MantisBTRssAddon,SvnRssAddon,GitRssAddon,HgRssAddon,WordPressRssAddon,TesztAddon</Ignore>
        <Directory>$home/.Schumix2/Addons</Directory>
    </Addons>
    <Scripts>
        <Lua>False</Lua>
        <Directory>$home/.Schumix2/Scripts</Directory>
    </Scripts>
    <Localization>
        <Locale>enUS</Locale>
    </Localization>
    <Update>
        <Enabled>False</Enabled>
        <Versions>
            <Enabled>False</Enabled>
            <Version>x.x.x</Version>
        </Versions>
        <WebPage>http://megax.uw.hu/Schumix2/</WebPage>
    </Update>
</Schumix>
